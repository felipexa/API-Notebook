@model IEnumerable<API_Locacao.Models.Devolucao>

@{
    ViewData["Title"] = "Index";
}

    <h1 class="text-center">DEVOLUÇÃO</h1>

<p>
    <a asp-action="Create">Devolver Produto</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Locacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Produto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Locacao.ClienteId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Produto.Modelo)
            </td>
            <td>
                
                <a asp-action="Details" asp-route-id="@item.DevolucaoId">Detalhes</a> 
               
            </td>
        </tr>
}
    </tbody>
</table>
